

build.img : sub/lib.fasl start.fasl BUILD.fasl
 ${imagedumper} build.img sub/lib.fasl start.fasl BUILD.fasl

sub/lib.fasl : sub/lib.lisp
 ${faslcompiler} sub/lib.fasl sub/lib.lisp

start.fasl : start.lisp
 ${faslcompiler} start.fasl start.lisp

BUILD.fasl : BUILD.lisp pkgdcl.fasl macros.fasl
 ${faslcompiler} BUILD.fasl BUILD.lisp pkgdcl.fasl macros.fasl

pkgdcl.fasl : pkgdcl.lisp
 ${faslcompiler} pkgdcl.fasl pkgdcl.lisp

macros.fasl : macros.lisp pkgdcl.fasl
 ${faslcompiler} macros.fasl macros.lisp pkgdcl.fasl

pkgdcl.fasl : pkgdcl.lisp
 ${faslcompiler} pkgdcl.fasl pkgdcl.lisp