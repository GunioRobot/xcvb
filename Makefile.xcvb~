

sub/lib.fasl : sub/lib.lisp
	/home/sbrody/xcvb/te\$$tsbcl/sbcl  --eval "(progn (compile-file \"sub/lib.lisp\") (sb-ext:quit))"

$tart.fasl : $tart.lisp
	/home/sbrody/xcvb/te\$$tsbcl/sbcl  --eval "(progn (compile-file \"\$$tart.lisp\") (sb-ext:quit))"

pkgdcl.fasl : pkgdcl.lisp
	/home/sbrody/xcvb/te\$$tsbcl/sbcl  --eval "(progn (compile-file \"pkgdcl.lisp\") (sb-ext:quit))"

//asdf/foo :
	/home/sbrody/xcvb/te\$$tsbcl/sbcl  --eval "(progn (asdf:oos 'asdf:load-op :foo) (sb-ext:quit))"



macros.fasl : macros.lisp pkgdcl.fasl //asdf/foo
	/home/sbrody/xcvb/te\$$tsbcl/sbcl  --eval "(progn (load \"pkgdcl.fasl\") (asdf:oos 'asdf:load-op :foo) (compile-file \"macros.lisp\") (sb-ext:quit))"

BUILD.fasl : BUILD.lisp pkgdcl.fasl //asdf/foo macros.fasl
	/home/sbrody/xcvb/te\$$tsbcl/sbcl  --eval "(progn (load \"pkgdcl.fasl\") (asdf:oos 'asdf:load-op :foo) (load \"macros.fasl\") (compile-file \"BUILD.lisp\") (sb-ext:quit))"

lisp : sub/lib.fasl $tart.fasl BUILD.fasl
	/home/sbrody/xcvb/te\$$tsbcl/sbcl  --eval "(progn (load \"sub/lib.fasl\") (load \"\$$tart.fasl\") (load \"BUILD.fasl\") (sb-ext:quit))"

IMAGE.img : lisp
	/home/sbrody/xcvb/te\$$tsbcl/sbcl  --eval "(progn (load \"sub/lib.fasl\") (load \"\$$tart.fasl\") (load \"BUILD.fasl\") (save-lisp-and-die \"IMAGE.img\"))"
